name "uim-graphs"
description "Graph/network library for modeling entities and relationships"
authors "UIM Team"
license "Apache-2.0"
version "0.1.0"
targetName "uim-graphs"
targetType "library"

configuration "default" {
    dependency "vibe-d" version="~>0.10.3"
}

configuration "modules" {
    versions "show_module"
}

configuration "tests" {
    targetType "executable"
    mainSourceFile "tests/graph_test.d"
    versions "show_module" "show_test"
}
